    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "aphrodite": "^2.4.0",
    "axios": "^0.20.0",
    "body-parser": "^1.19.0",
    "concurrently": "^5.3.0",
    "cookie-parser": "^1.4.5",
    "cross-env": "^7.0.2",
    "electron-is-dev": "^1.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "mongodb": "^3.6.1",
    "node-sass": "^4.14.1",
    "react": "^16.13.1",
    "react-animations": "^1.0.0",
    "react-contenteditable": "^3.3.5",
    "react-dom": "^16.13.1",
